public class LoopBreak {
	public static void main(String[] args) {
		for (int i = 0; i < 10; i++) {
			if (i == 5) {
				break;
			}
			System.out.println(i);
		}
		System.out.println("프로그램 종료");
		
		for (int i = 10; i < 20; i++) {
			if (i % 10 == 5) {
				continue; // 현재 단계를 건너뛰어라
			}
			System.out.println(i);
		}
		System.out.println("프로그램 종료");
	}
}